<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="Login_root" stylesheets="@loginwindow.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shoreline.GUI.Controller.LoggedInWindowController">
   <children>
      <Button fx:id="LogOutBtn" layoutX="480.0" layoutY="353.0" mnemonicParsing="false" onAction="#handleCancel" styleClass="Buttons_Small" text="Log-Out" />
      <Button fx:id="SavedPatternBtn" layoutX="14.0" layoutY="311.0" mnemonicParsing="false" onAction="#handleExistingPattern" styleClass="Buttons_Small" text="Convert with existing Pattern" />
      <Button fx:id="ViewLogBtn" layoutX="14.0" layoutY="353.0" mnemonicParsing="false" onAction="#handleLog" styleClass="Buttons_Small" text="View Log" />
      <Label layoutX="14.0" layoutY="14.0" styleClass="Labels_big" text="Task Log:" />
      <TableView fx:id="taskTable" layoutX="14.0" layoutY="49.0" prefHeight="251.0" prefWidth="240.0">
        <columns>
          <TableColumn fx:id="patternClm" prefWidth="124.5" text="Used Pattern" />
          <TableColumn fx:id="pathClm" prefWidth="114.5" text="Path" />
        </columns>
      </TableView>
      <Label layoutX="364.0" layoutY="17.0" styleClass="Labels_small" text="Logged-in as:" textAlignment="RIGHT" />
      <Label layoutX="363.0" layoutY="35.0" styleClass="Labels_small" text="Current Date:" textAlignment="RIGHT" />
      <Label fx:id="loggedInlbl" layoutX="480.0" layoutY="17.0" styleClass="Labels_small" text="asd" textAlignment="RIGHT" />
      <Label fx:id="dateLbl" layoutX="480.0" layoutY="35.0" styleClass="Labels_small" text="asd" textAlignment="RIGHT" />
      <Button fx:id="NewPatternBtn" layoutX="359.0" layoutY="311.0" mnemonicParsing="false" onAction="#handleNewPattern" styleClass="Buttons_Small" text="Convert with new Pattern" />
      <Button fx:id="taskRepeate" layoutX="281.0" layoutY="357.0" mnemonicParsing="false" onAction="#handleTaskRepeate" text="WIP" />
   </children>
</AnchorPane>
