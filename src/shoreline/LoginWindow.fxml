<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="400.0" styleClass="Login_root" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shoreline.LoginWindowController">
    <stylesheets>
        <URL value="@loginwindow.css" />
    </stylesheets>
   <children>
      <Label layoutX="145.0" layoutY="117.0" styleClass="Labels" text="User Name:" />
      <Label layoutX="152.0" layoutY="180.0" styleClass="Labels" text="Password:" />
      <TextField fx:id="txt_id" layoutX="113.0" layoutY="138.0" prefHeight="25.0" prefWidth="175.0" />
      <TextField fx:id="txt_pw" layoutX="113.0" layoutY="201.0" prefHeight="25.0" prefWidth="175.0" />
      <Button fx:id="btn_Login" layoutX="14.0" layoutY="248.0" mnemonicParsing="false" onAction="#handleLogin" styleClass="Buttons_Big" text="Login" />
      <Button fx:id="btn_Cacel" layoutX="280.0" layoutY="248.0" mnemonicParsing="false" onAction="#handleCancel" styleClass="Buttons_Big" text="Cancel" />
   </children>
</AnchorPane>
